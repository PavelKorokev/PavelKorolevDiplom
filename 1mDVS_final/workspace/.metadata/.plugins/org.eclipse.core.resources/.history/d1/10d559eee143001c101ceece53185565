package PID_Regulator;
import SystemLib.Delay.DelayValue;

class Component {
	real pPressureError;
	real qCoeff_P;
	real cPID_P_Max;
	real cPID_P_Min;
	real cPID_I_Max;
	real cPID_I_Min;
	real qCoeff_I;
	real pMemory_I;
	DelayValue DelayValue_instance;
	real qCoeff_D;
	real cPID_D_Min;

	@generated("blockdiagram", "1479c27e")
	@error("blockdiagram")
	public void Input(real in pPressureSet, real in pPressure) {
		pPressureError = (pPressureSet - pPressure); // Main/Input 1
		DelayValue_instance.compute(pPressureError); // Main/Input 2
	}

	@generated("blockdiagram", "01096e72")
	@error("blockdiagram")
	public real Out() {
	}

	@generated("blockdiagram", "4937bb8f")
	@error("blockdiagram")
	public void Input_I() {
		pMemory_I = max(cPID_I_Max, min((pPressureError + pMemory_I), cPID_I_Min)); // Main/Input_I 1
	}

	@generated("blockdiagram", "9119d744")
	@error("blockdiagram")
	public void PID(real in cPID_D_Max) {
	}
}